<view class="root" bind:__l="__l"><view class="head-nav"><text class="head-text">我的</text><image class="head-notification" src="../../static/icons/notification.png" data-event-opts="{{[['tap',[['goMessage',['$event']]]]]}}" bindtap="__e"></image><image class="head-setting" src="../../static/icons/setting1.png" data-event-opts="{{[['tap',[['goSetting',['$event']]]]]}}" bindtap="__e"></image></view><view class="center"><view class="logo-backgroud"><view class="logo"><view data-event-opts="{{[['tap',[['goOther',['$event']]]]]}}" class="img" bindtap="__e"><block wx:if="{{!hasLogin}}"><image class="logo-img" hover-class="logo-hover" src="/static/icons/logo.png" data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" bindtap="__e"></image></block><block wx:else><image class="logo-img" src="{{userInfo.headImageUrl}}"></image></block></view><view class="logo-title"><block wx:if="{{!hasLogin}}"><view data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" class="user-name" bindtap="__e">登录</view></block><block wx:else><view class="user-name">{{userInfo.name}}</view></block><block wx:if="{{hasLogin}}"><view data-event-opts="{{[['tap',[['goPoints',['$event']]]]]}}" class="certify" bindtap="__e"><block wx:if="{{userInfo.isCertified}}"><view class="certify-text">已认证</view></block><block wx:else><view class="certify-text">未认证</view></block></view></block></view></view><view><image class="logo-edit" src="../../static/icons/edit_information.png" data-event-opts="{{[['tap',[['goEditInformation']]]]}}" bindtap="__e"></image></view><view data-event-opts="{{[['tap',[['goPoints',['$event']]]]]}}" class="points" bindtap="__e"><block wx:if="{{userInfo.points}}"><text class="point-text">{{"$"+pointText+"积分"}}</text></block><block wx:else><text class="point-text">$0积分</text></block></view></view><view class="body"><view class="basic-data _div"><view data-event-opts="{{[['tap',[['goFollow',['$event']]]]]}}" class="basic-data-item _div" bindtap="__e"><text>关注</text><text>{{userInfo.followNum}}</text></view><view data-event-opts="{{[['tap',[['goCollect',['$event']]]]]}}" class="basic-data-item _div" bindtap="__e"><text>收藏</text><text>{{userInfo.collectPostNum}}</text></view><view data-event-opts="{{[['tap',[['goPost',['$event']]]]]}}" class="basic-data-item _div" bindtap="__e"><text>帖子</text><text>{{userInfo.postNum}}</text></view><view data-event-opts="{{[['tap',[['goComment',['$event']]]]]}}" class="basic-data-item-last _div" bindtap="__e"><text>评论</text><text>{{userInfo.commentNum}}</text></view></view></view><view class="image-con"><view class="left-up"><image class="left-up-img" src="../../static/images/FZU-scenery-1.jpg"></image></view><view class="right-up"><image class="right-up-img" src="../../static/images/FZU-scenery-2.jpg"></image></view></view><view class="image-con"><view class="left-bottom"><image class="left-bottom-img" src="../../static/images/FZU-scenery-3.jpg"></image></view><view class="right-bottom"><image class="right-bottom-img" src="../../static/images/FZU-scenery-4.jpg"></image></view></view></view></view>