<view id="change-userInfo" bind:__l="__l"><image class="background-img" src="{{userInfo.backgroundImg}}"></image><view class="userInfo"><image class="userInfo-head-img" src="{{userInfo.headImg}}"></image><view class="userInfo-top"><view class="userInfo-top-label">全部信息</view><image class="userInfo-top-icon" src></image></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">姓名</view><input class="userInfo-item-edit-input" type="text" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['userInfo']]]]]}}" value="{{userInfo.name}}" bindinput="__e"/></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">邮箱</view><input class="userInfo-item-edit-input" type="text" data-event-opts="{{[['input',[['__set_model',['$0','mail','$event',[]],['userInfo']]]]]}}" value="{{userInfo.mail}}" bindinput="__e"/></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">手机号</view><input class="userInfo-item-edit-input" type="text" data-event-opts="{{[['input',[['__set_model',['$0','phoneNum','$event',[]],['userInfo']]]]]}}" value="{{userInfo.phoneNum}}" bindinput="__e"/></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">密码</view><input class="userInfo-item-edit-input" type="text" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['userInfo']]]]]}}" value="{{userInfo.password}}" bindinput="__e"/></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">更改学号绑定</view><input class="userInfo-item-edit-input" type="text" data-event-opts="{{[['input',[['__set_model',['$0','studentNum','$event',[]],['userInfo']]]]]}}" value="{{userInfo.studentNum}}" bindinput="__e"/></view><view class="devide-line"></view></view></view>