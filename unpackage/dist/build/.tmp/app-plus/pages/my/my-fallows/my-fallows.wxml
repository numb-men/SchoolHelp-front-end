<view class="content" bind:__l="__l"><block wx:for="{{myFollows}}" wx:for-item="myFollow" wx:for-index="index" wx:key="id"><block><view class="my-follow-item" id="{{myFollow.id}}" data-index="{{index}}"><view class="left"><image class="user-head-img" src="{{myFollow.userHeadImg}}"></image><view class="my-follow-item-center"><block wx:if="{{myFollow.isCertified}}"><view class="has-certified">已认证</view></block><block wx:else><view class="not-certified">未认证</view></block><view class="user-name">{{myFollow.name}}</view></view></view><view class="my-follow-item-right"><image class="normal-icon" src="/static/icons/more.png"></image><image class="normal-icon" src="/static/icons/cancel-follow.png"></image></view></view><view class="devide-line"></view></block></block></view>