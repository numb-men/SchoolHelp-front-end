<view id="change-userInfo" bind:__l="__l"><image class="background-img" src="../../../../static/images/my_background.jpg"></image><view class="userInfo"><image class="userInfo-head-img" src="{{userInfo.headImageUrl}}" data-event-opts="{{[['tap',[['goUploadImage',['$event']]]]]}}" bindtap="__e"></image><view class="userInfo-top"><view class="userInfo-top-label">全部信息</view><image class="userInfo-top-icon" src></image></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">昵称</view><input class="userInfo-item-edit-input" type="text" maxlength="13" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['userInfo']]]]]}}" value="{{userInfo.name}}" bindinput="__e"/></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">手机号</view><input class="userInfo-item-edit-input" type="text" maxlength="11" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['userInfo']]]]]}}" value="{{userInfo.phone}}" bindinput="__e"/></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">性别</view><input class="userInfo-item-edit-input" type="text" maxlength="1" data-event-opts="{{[['input',[['__set_model',['$0','sex','$event',[]],['userInfo']]]]]}}" value="{{userInfo.sex}}" bindinput="__e"/></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">学号</view><input class="userInfo-item-edit-input" type="text" maxlength="9" data-event-opts="{{[['input',[['__set_model',['$0','studentNum','$event',[]],['userInfo']]]]]}}" value="{{userInfo.studentNum}}" bindinput="__e"/></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">专业</view><input class="userInfo-item-edit-input" type="text" maxlength="13" data-event-opts="{{[['input',[['__set_model',['$0','major','$event',[]],['userInfo']]]]]}}" value="{{userInfo.major}}" bindinput="__e"/></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">学院</view><input class="userInfo-item-edit-input" type="text" maxlength="13" data-event-opts="{{[['input',[['__set_model',['$0','college','$event',[]],['userInfo']]]]]}}" value="{{userInfo.college}}" bindinput="__e"/></view><view class="devide-line"></view><view class="userInfo-item"><view class="userInfo-item-label">邮箱</view><input class="userInfo-item-edit-input" type="text" maxlength="25" data-event-opts="{{[['input',[['__set_model',['$0','mail','$event',[]],['userInfo']]]]]}}" value="{{userInfo.mail}}" bindinput="__e"/></view><view class="devide-line"></view><view class="userInfo-item"><button class="primary" type="primary" formType="submit" data-event-opts="{{[['tap',[['saveChange']]]]}}" bindtap="__e">保存修改</button></view></view></view>